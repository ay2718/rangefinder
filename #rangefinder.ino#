#include "rangefinder.h"
#include "display.h"

// trigger pin, echo pin
Rangefinder range(7, 6);

Display disp(2, 3, 4, 5, 8, 9, 10, 11);

void setup() {
  Serial.begin (9600);
}

void loop() {
  // grabs a pulse
  range.pulse();
  // how to use the distance
  float distance = range.get();
  // prints it
  range.print();
  delay(100);
}
